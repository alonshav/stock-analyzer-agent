{
  "$schema": "https://railway.com/railway.schema.json",
  "build": {
    "builder": "DOCKERFILE",
    "dockerfilePath": ".railway/agent/Dockerfile"
  },
  "deploy": {
    "startCommand": "node dist/apps/agent/main.js --skip-nx-cache",
    "restartPolicyType": "ON_FAILURE",
    "restartPolicyMaxRetries": 10
  }
}
